apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginxatc
  namespace: atcprojectdeploy
spec:
  replicas: 3
  selector:
    matchLabels:
      app: nginx
  template:
    metadata:
      labels:
        app: nginx
    spec:
      containers:
      - name: nginx
        image: rockiee/atcprojectngnix:test
        ports:
        - containerPort: 80
